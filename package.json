  },
  "homepage": "https://github.com/VanBelleKoen/cypress_soft_assert#readme",
  "peerDependencies": {
    "cypress": ">=10.0.0"
  },
  "devDependencies": {
    "cypress": "^15.9.0",
    "typescript": "^5.9.3",
    "@types/mocha": "^10.0.10"
  },
  "files": [
    "dist",
